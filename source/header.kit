        <script>
          var style_cookie = document.cookie.match(/style=((black)|(white))/);
          var theme = "white";
          if (style_cookie === null) {
              var match = window.matchMedia('(prefers-color-scheme: dark)');
              theme = (match && match.matches) ? "black" : "white";
          } else {
              theme = style_cookie[1]
          }
          document.write(
              "<link rel='stylesheet' href='/css/"
                  + theme + ".css' media='screen' id='theme'>");
        </script>
